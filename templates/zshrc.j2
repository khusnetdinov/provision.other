ZSH=$HOME/.oh-my-zsh
ZSH_THEME="robbyrussell"

plugins=(history-substring-search ruby rails gem git git-flow rake bundler git-extras cp)

source $ZSH/oh-my-zsh.sh
export PATH=/opt/local/bin:/opt/local/sbin:/usr/local/bin:/usr/local/sbin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/bin:/usr/X11/bin
export PATH="$HOME/bin:$HOME/.rbenv/bin:$PATH:/usr/local/share/npm/bin/"
